---
import BaseLayout from "@/layouts/BaseLayout.astro";
import SidebarLayout from "@/layouts/sidebarLayout";
import JsonToPhp from "@/components/JsonToPhp";

const breadcrumbItems = [
    { title: "Home", href: "/" },
    { title: "Data Conversion", href: "/data-conversion" },
    { title: "JSON to PHP" },
];
---

<BaseLayout title="JSON to PHP">
	<SidebarLayout client:load header={breadcrumbItems}>
		<JsonToPhp client:load />
	</SidebarLayout>
</BaseLayout>