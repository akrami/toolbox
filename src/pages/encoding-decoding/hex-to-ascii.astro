---
import BaseLayout from "@/layouts/BaseLayout.astro";
import SidebarLayout from "@/layouts/sidebarLayout";
import HexToAscii from "@/components/HexToAscii";

const breadcrumbItems = [
    { title: "Home", href: "/" },
    { title: "Encoding & Decoding", href: "/encoding-decoding" },
    { title: "HEX/ASCII Converter" },
];
---

<BaseLayout title="HEX/ASCII Converter">
	<SidebarLayout client:load header={breadcrumbItems}>
		<HexToAscii client:load />
	</SidebarLayout>
</BaseLayout>