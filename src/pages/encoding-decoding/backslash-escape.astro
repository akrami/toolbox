---
import BaseLayout from "@/layouts/BaseLayout.astro";
import SidebarLayout from "@/layouts/sidebarLayout";
import BackslashEscape from "@/components/BackslashEscape";

const breadcrumbItems = [
    { title: "Home", href: "/" },
    { title: "Encoding & Decoding", href: "/encoding-decoding" },
    { title: "Backslash Escape" },
];
---

<BaseLayout title="Backslash Escape Encoder/Decoder">
	<SidebarLayout client:load header={breadcrumbItems}>
		<BackslashEscape client:load />
	</SidebarLayout>
</BaseLayout>