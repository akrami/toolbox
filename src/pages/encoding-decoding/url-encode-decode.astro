---
import BaseLayout from "@/layouts/BaseLayout.astro";
import SidebarLayout from "@/layouts/sidebarLayout";
import UrlEncodeDecode from "@/components/UrlEncodeDecode";

const breadcrumbItems = [
    { title: "Home", href: "/" },
    { title: "Encoding & Decoding", href: "/encoding-decoding" },
    { title: "URL" },
];
---

<BaseLayout title="URL Encode/Decode">
	<SidebarLayout client:load header={breadcrumbItems}>
		<UrlEncodeDecode client:load />
	</SidebarLayout>
</BaseLayout>